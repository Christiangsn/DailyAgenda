{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to dev:debug",
            "protocol": "inspector",
            "port": 9229,
            "restart": true,
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug NestJS App",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
              "run-script",
              "debug"
            ]
        }
    ]
}