{
  "name": "daily-agenda",
  "version": "0.0.0.1",
  "description": "",
  "author": "Christian Guimar√£es",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "npx eslint --fix src",
    "start:debug-local": "nest start --debug --watch tsconfig.json",
    "start:local": "nest start --watch",
    "start": "nest start",
    "test": "npx jest --setupFiles --passWithNoTests --no-cache --runInBand --detectOpenHandles --forceExit --silent",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npx jest --config=./jest.config.js --coverage --passWithNoTests --no-cache --runInBand --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand -c ./jest.config.js",
    "test:ci-integration": "npx jest --config=./jest.integration.config.js --coverage --runInBand --passWithNoTests --no-cache --detectOpenHandles --forceExit",
    "test:integration": "npx jest -c ./jest.integration.config.js --runInBand --detectOpenHandles --no-cache --forceExit",
    "test:debug:integration": "npm run && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand -c ./jest.integration.config.js",
    "test:html-report": "npx jest --config=./jest.config.coveralls.js --coverage --passWithNoTests --no-cache --runInBand --detectOpenHandles --forceExit && coveralls < coverage/lcov.info",
    "generate:keys": "npx ts-node-dev ./src/@config/env-token.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^9.0.0",
    "@types/uuid": "^9.0.4",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.3",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "29.5.0",
    "jest-html-reporter": "^3.10.2",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.5",
    "mongodb-memory-server": "^8.15.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": "18.x"
  }
}
